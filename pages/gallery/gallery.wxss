@import '../../components/nullTip/nullTip.wxss';
.gallery{
  display: flex;
  flex-wrap: wrap;
  margin-bottom: 60rpx;
  position: relative;
  width: 100%;
}
.gallery-item{
  border-right: solid 1px #e5e5e5;
  border-bottom: solid 1px #e5e5e5;
  box-sizing: border-box;
  width: 250rpx;
  height: 250rpx;
  overflow: hidden;
  position: relative;
}
.gallery-item-cover{
  width: 100%;
  height: 100%;
}